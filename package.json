{
	"name": "emcee-tournament-bot",
	"version": "0.1.0",
	"description": "A Discord bot to facilitate organising Challonge tournaments and verify submitted decks.",
	"private": true,
	"main": "dist/index.js",
	"scripts": {
		"test": "nyc mocha --config test/.mocharc.yml test/*.ts test/**/*.unit.ts",
		"cover": "nyc report",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"build": "tsc",
		"delete": "ts-node src/cli/delete",
		"delete:docker": "node --enable-source-maps --unhandled-rejections=strict cli/delete",
		"leave": "ts-node src/cli/leave",
		"leave:docker": "node --enable-source-maps --unhandled-rejections=strict cli/leave"
	},
	"author": "Luna Brand",
	"contributors": [
		"Kevin Lu"
	],
	"license": "AGPL-3.0-or-later",
	"engines": {
		"node": ">=14.x",
		"yarn": "1.x"
	},
	"dependencies": {
		"@fast-csv/format": "^4.3.5",
		"debug": "^4.3.1",
		"dotenv": "10.0.0",
		"eris": "^0.15.0",
		"node-fetch": "^2.6.1",
		"pg": "^8.6.0",
		"reflect-metadata": "^0.1.13",
		"supports-color": "^9.0.0",
		"typeorm": "^0.2.32",
		"ydeck": "^0.2.2",
		"ydke": "^1.1.0",
		"ygopro-data": "1.3.6"
	},
	"devDependencies": {
		"@alphakretin/essentials": "../typescript-tooling/essentials",
		"@alphakretin/mocha-ecosystem": "../typescript-tooling/mocha-ecosystem",
		"@types/debug": "^4.1.5",
		"@types/node-fetch": "^2.5.10"
	},
	"eslintConfig": {
		"extends": "@alphakretin"
	},
	"prettier": "@alphakretin/essentials",
	"nyc": {
		"extends": "@alphakretin/mocha-ecosystem"
	}
}
